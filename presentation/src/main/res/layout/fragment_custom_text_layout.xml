<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.example.presentation.fragment.customtextview.ReviewView
        android:id="@+id/myTextView"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="#DDE8AD"
        android:gravity="center_horizontal"
        android:maxWidth="200dp"
        android:maxLines="4"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:subTextColor="#39A2D1"
        app:subTextLines="3"
        app:subTextMarginTop="10dp"
        app:subTextSize="13sp" />

    <com.example.presentation.fragment.customtextview.ReviewView
        android:id="@+id/myTextView4"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="#DDE8AD"
        android:gravity="center_horizontal"
        android:maxWidth="200dp"
        android:maxLines="4"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@id/myTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:subTextColor="#39A2D1"
        app:subTextLines="3"
        app:subTextMarginTop="0dp"
        app:subTextSize="13sp" />

    <com.example.presentation.fragment.customtextview.ReviewView
        android:id="@+id/myTextView5"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="#DDE8AD"
        android:gravity="center_horizontal"
        android:maxWidth="200dp"
        android:maxLines="4"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@id/myTextView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:subTextColor="#39A2D1"
        app:subTextLines="3"
        app:subTextMarginTop="0dp"
        app:subTextSize="13sp" />

    <TextView
        android:id="@+id/tete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="동해물과 백두산이 "
        android:textSize="16sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/myTextView" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="동해물과 백두산이 "
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tete" />

    <com.example.presentation.fragment.customtextview.ReviewView
        android:id="@+id/myTextView2"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="#DDE8AD"
        android:gravity="center_horizontal"
        android:maxWidth="200dp"
        android:maxLines="4"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/myTextView"
        app:subTextColor="#39A2D1"
        app:subTextLines="3"
        app:subTextMarginTop="10dp"
        app:subTextSize="13sp" />

    <com.example.presentation.fragment.customtextview.ReviewView
        android:id="@+id/myTextView3"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:background="#DDE8AD"
        android:gravity="center_horizontal"
        android:maxWidth="200dp"
        android:maxLines="4"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/myTextView2"
        app:subTextColor="#39A2D1"
        app:subTextLines="3"
        app:subTextSize="13sp" />
</androidx.constraintlayout.widget.ConstraintLayout>